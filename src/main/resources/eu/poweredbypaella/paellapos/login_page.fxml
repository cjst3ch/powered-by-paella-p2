  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="515.0" layoutY="242.0" text="Ramon Villa" textAlignment="CENTER">
               <font>
                  <Font size="40.0" />
               </font>
            </Label>
            <TextField fx:id="username" layoutX="546.0" layoutY="340.0" promptText="Employee ID" />
            <Button fx:id="loginButton" layoutX="596.0" layoutY="381.0" mnemonicParsing="false" onAction="#onLoginClick" text="Log in" />
      </children>
    </AnchorPane>
  </children>
   <padding>
